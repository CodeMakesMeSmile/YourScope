<ngb-carousel>
	<ng-template ngbSlide *ngFor="let element of elements">
		<div class="picsum-img-wrapper">
			<img [src]="element.imagePath" alt="{{element.imageAlt}}" />
		</div>
		<div class="carousel-caption">
			<h3>{{element.captionHeading}}</h3>
			<p>{{element.captionSubheading}}</p>
		</div>
	</ng-template>
</ngb-carousel>
	